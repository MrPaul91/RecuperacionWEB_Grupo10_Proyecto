###  http://dbpedia.org/ontology/Company
dbo:Company rdf:type owl:Class .


###  http://dbpedia.org/ontology/Person
dbo:Person rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :canDoTask ;
                             owl:allValuesFrom dbo:Task
                           ] ;
           owl:disjointWith :Competence .


###  http://dbpedia.org/ontology/Skill
dbo:Skill rdf:type owl:Class ;
          owl:equivalentClass :Competence ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTask ;
                            owl:someValuesFrom :Task
                          ] .


###  http://dbpedia.org/ontology/Task
dbo:Task rdf:type owl:Class ;
         owl:equivalentClass :Task .


###  http://www.entrega1/ontologies/Competence
:Competence rdf:type owl:Class ;
            owl:disjointWith :Task .


###  http://www.entrega1/ontologies/PersonWithJob
:PersonWithJob rdf:type owl:Class ;
               rdfs:subClassOf [ owl:intersectionOf ( dbo:Person
                                                      [ rdf:type owl:Class ;
                                                        owl:complementOf :PersonWithoutJob
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
               owl:disjointWith :PersonWithoutJob .


###  http://www.entrega1/ontologies/PersonWithoutJob
:PersonWithoutJob rdf:type owl:Class ;
                  rdfs:subClassOf dbo:Person .


###  http://www.entrega1/ontologies/Task
:Task rdf:type owl:Class .


###  http://www.entrega1/ontologies/jobPosition
:jobPosition rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :needsCompetence ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Competence
                             ] .


